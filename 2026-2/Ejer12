import java.util.*;

public class Ejercicio12 {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int[] A=new int[n];

        for(int i=0;i<n;i++)
            A[i]=sc.nextInt();

        int x=sc.nextInt();
        ArrayList<Integer> pos=new ArrayList<>();

        for(int i=0;i<n;i++)
            if(A[i]==x) pos.add(i+1);

        System.out.println("Posiciones: "+pos);
    }
}
