import java.util.Scanner;

public class Ejercicio14 {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int[] A=new int[n];

        for(int i=0;i<n;i++)
            A[i]=sc.nextInt();

        int[] B=new int[n-1];

        for(int i=0;i<n-1;i++)
            B[i]=A[i]-A[i+1];

        for(int i=0;i<n-1;i++)
            System.out.print(B[i]+" ");
    }
}
