import java.util.Scanner;

public class Ejercicio10 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        int[] A=new int[n];

        for(int i=0;i<n;i++)
            A[i]=sc.nextInt();

        int mitad=(n+1)/2;
        int[] B=new int[mitad];

        for(int i=0;i<mitad;i++)
            B[i]=A[i]+A[n-1-i];

        for(int i=0;i<mitad;i++)
            System.out.print(B[i]+" ");
    }
}
